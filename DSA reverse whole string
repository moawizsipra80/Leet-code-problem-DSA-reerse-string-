class Solution {
public:
    string reverseWords(string s) {
    stringstream ss(s);
    string words;
    vector<string>results;
    string result;
    while(ss>>words){

    results.push_back(words);

    }
        reverse(results.begin(),results.end());

    for(int i=0;i<results.size();i++)
    {
        if(i>=0)
         {
    result+=" ";

        }
            result+=results[i];
 }
 return result;   
    }
};
